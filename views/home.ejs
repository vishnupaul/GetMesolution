<div class="row mt-5">
  <div class="col-md-6 m-auto">
    <div class="card card-body text-center">
      <h1>Welcome</h1>
      <h2>To</h2>
      <h2>SolutionWeb</h2>
      <h1>SolutionWeb / <%= title %></h1>
    </div>
  </div>
</div>
<div id="feed-posts">
  <% if(locals.user){%>
  <form action="/feedbacks/create" id="new-feedback-form" method="POST">
    <fieldset class="mt-2">
      <div class="form-group">
        <label for="feedbacks" class="form-label mt-4">Feedbacks Us</label>
            <textarea
              class="form-control"
              name="content"
              id="feedbacks"
              rows="3"
              placeholder="Feedbacks Us Here...."
              spellcheck="false"
            ></textarea>
      </div>
      <input type="submit" value="Feedbacks Us" class="btn btn-primary mt-2" />
    </fieldset>
  </form>
  <%}%>
</div>

<div class="mx-4 mt-4">

  <h3>Feedbacks</h3>
  <di class="d-flex" id="feedback-container">
      <% for(feedback of feedbacks){%> 
        
        <%- include('_feedback') -%>
      <%}%>
</div>
<div class="mx-4 mt-4">

  <h3>Projects</h3>
    <div class="d-flex" id="project-container">
  
      <%- include('_project') -%>
        
    </div>
</div>
<script src="/js/home_feedback.js"></script>
<script src="/js/home_project.js"></script>
